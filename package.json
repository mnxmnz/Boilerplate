{
  "name": "my-own-boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnxmnz/create-my-own-boilerplate"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "yarn lint:eslint",
      "yarn lint:prettier",
      "yarn lint:stylelint"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --fix \"./**/*.{js,ts,tsx}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --write \"./**/*.{js,ts,tsx}\" --ignore-path .gitignore",
    "lint:stylelint": "stylelint --fix \"./**/*.tsx\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@tanstack/react-query": "^4.26.1",
    "axios": "^1.3.4",
    "emotion-normalize": "^11.0.1",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.2.4",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-concentric-order": "^5.1.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
